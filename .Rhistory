arrows(a_1_comb[1],a_1_comb[2],(a_1_comb+a_2_comb)[1],(a_1_comb+a_2_comb)[2], col="green", lty="dashed")
}
arrows(0,0, b[1], b[2], col="blue")
legend(x_min, y_max, legend=c("Original vector", "Multiplied vector", "Result"),
